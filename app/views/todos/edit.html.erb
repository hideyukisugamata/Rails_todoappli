<div class="col-md-4 offset-md-4">
<h1 class="text-center">Input Edit Todo </h1>
<%= form_with model:@edit_todo_list, url:todo_update_path,local:true do |f| %>
  <div class="form-group">
    <label>やること</label>
    <%= f.text_field :list,class:"form-control" %>
  </div>
  <div class="text-center">
    <%= f.submit "編集",class:"btn btn-primary"%>
  </div>
<% end %>