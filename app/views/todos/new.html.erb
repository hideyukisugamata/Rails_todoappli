<div class="col-md-4 offset-md-4">
<h1 class="text-center">Input New Todo </h1>
<%= form_with model:@new_todo_list, url:todo_create_path,local:true do |f| %>
  <div class="form-group">
    <label>やること</label>
    <%= f.text_field :list,class:"form-control" %>
  </div>
  <div class="text-center">
    <%= f.submit "登録",class:"btn btn-primary"%>
  </div>
<% end %>